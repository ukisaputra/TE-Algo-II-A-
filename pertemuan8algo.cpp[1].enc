using namespace::std;
double input(string text){
    double nilai;
cout<<"Input nilai "<<text<<" : ";cin>>nilai;
return nilai;
}
void grade (double nilai){
    if(nilai>81){cout<<"nilai anda a";}
    else if(nilai>61){cout<<"nilai anda b";}
    else if(nilai>41){cout<<"nilai anda c";}
   else if(nilai>21){cout<<"nilai anda d";}
    else {cout<<"nilai anda e";}
}
main(){

   double uts,uas,tugas,absen,nilai;

    uts=input("uts");
    uas=input("uas");
    tugas=input("Tugas");
    absen=input("Absen");
nilai=(uts*20/100)+(uas*30/100)+(tugas*35/100)+(absen*15/100);
cout<<"nilai "<<nilai<<endl;
grade(nilai);
}
